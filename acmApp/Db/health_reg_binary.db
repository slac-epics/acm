record(bi, "$(P)$(REG)") {
    field(DTYP, "ACM Reg Value")
    field(INP , "@$(DEV) cmd=0xAE offset=$(OFF) mask=$(MASK=0xffffffff) shift=$(SHIFT=0)")
    field(SCAN, "$(SCAN=I/O Intr)")
    field(ZNAM, "$(ZNAM=Zero)")
    field(ZSV, "$(ZSV=NO_ALARM)")
    field(ONAM, "$(ONAM=One)")
    field(OSV, "$(OSV=MAJOR)")
    field(TSE , "-2") # TIME from driver
    field(FLNK, "$(FLNK=)")
}
